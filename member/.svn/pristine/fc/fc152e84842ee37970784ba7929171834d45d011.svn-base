package com.taole.member.domain.param.ShopInfoService;

import java.util.Date;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

public class Query {
	@ApiModel(value = "查询店面信息列表请求参数")
	public static class ShopInfoServiceQueryReq {

		@ApiModelProperty(value = "店面名称", example = "国贸店")
		private String name;

		@ApiModelProperty(value = "省份", example = "310000")
		private String province;

		@ApiModelProperty(value = "城市", example = "310104")
		private String city;

		@ApiModelProperty(value = "状态(已开业1/未开业0)")
		private String status;
		
		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}

		public String getProvince() {
			return province;
		}

		public void setProvince(String province) {
			this.province = province;
		}

		public String getCity() {
			return city;
		}

		public void setCity(String city) {
			this.city = city;
		}

		public String getStatus() {
			return status;
		}

		public void setStatus(String status) {
			this.status = status;
		}

	}

