.char-item { 
    float: left;  
    border: 1px solid #99BBE8;  
    margin: 3px;  
    text-align: center;  
    vertical-align: middle;  
    font-size: 14px; 
    color: #15428B; 
    cursor: pointer; 
} 
 
.char-item.x-item-selected { 
    background-color: #777; 
} 
 
.char-over { 
    border: 1px solid #15428B; 
    background-color:#d0def0; 
} 
